    "pwnutil",
    "pwntools"
  ],
  "author": "aplet123",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aplet123/pwnutil/issues"
  },
  "homepage": "https://github.com/Aplet123/pwnutil#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "typedoc": "^0.17.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/node": "^13.9.1",
    "chalk": "^3.0.0",
    "minimist": "^1.2.6"
  }
