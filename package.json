{
  "name": "pwnutil",
  "version": "0.0.1",
  "description": "Pwntools but in JS.",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"no test\"",
    "build": "tsc",
    "export": "(node dist/src/genExports.js > src/index.ts) && tsc",
